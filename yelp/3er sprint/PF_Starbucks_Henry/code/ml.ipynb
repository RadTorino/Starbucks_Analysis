   "source": [
    "# Cargar el archivo reviews_starbucks.parquet\n",
    "file_path = '../gcp/reviews_starbucks.parquet'\n",
    "df_reviews = pd.read_parquet(file_path)\n",
    "\n",
    "# Mostrar una vista previa de los datos\n",
    "df_reviews.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Generar etiquetas aleatorias para la detecci칩n de reviews falsos\n",
    "np.random.seed(42)\n",
    "df_reviews['label'] = np.random.randint(0, 2, df_reviews.shape[0])\n",
    "\n",
    "# Separar caracter칤sticas y etiquetas, usando solo text y stars\n",
    "X = df_reviews[['text', 'stars']]\n",
    "y = df_reviews['label']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Vectorizar el texto de las reviews\n",
    "tfidf = TfidfVectorizer(stop_words='english', max_features=100)\n",
    "X_tfidf = tfidf.fit_transform(X['text'])\n",
    "\n",
    "# Combinar las caracter칤sticas textuales con la columna stars\n",
    "X_combined = np.hstack((X_tfidf.toarray(), X[['stars']].values.reshape(-1, 1)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Dividir el conjunto de datos en entrenamiento y prueba\n",
    "X_train, X_test, y_train, y_test = train_test_split(X_combined, y, test_size=0.2, random_state=42)"
   ]
  },
